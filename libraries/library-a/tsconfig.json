{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ES2020",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "checkJs": false,
  },
  "exclude": [
    "dist/**/*",
    "node_modules/**/*"
  ]
}